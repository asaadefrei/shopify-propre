    var Section1Box4Img2P4 = document.querySelector('.Section1Box4Img2P4');
    var Section1Box4Img3P4 = document.querySelector('.Section1Box4Img3P4');


    

  

    document.getElementById('1-4box-first-Link-produit-1').addEventListener('click', function() {
            event.preventDefault();
   {% for block in section.blocks %}
            {% if block.type == "produit_1" %}
              {% assign imageProduit = block.settings.imageProduit %}
              {% assign urlProduit = block.settings.selected_product.url %}
              {% assign product = block.settings.selected_product %}
              document.getElementById('littleQuatreBox1Titre1').innerHTML =  "{{ block.settings.titre  }}"

         Section1Box4LinkBox1.href = "{{ urlProduit }}";

         Section1Box4ImageProduitBox1.src = '{{imageProduit | img_url: 'master'}}';
          Section1quatreBoxWishP1B1.style.display = '';
            Section1quatreBoxWishP1B2.style.display = 'none';

           Section1quatreBoxWishP1B3.style.display = 'none';      

            {% endif %}
          {% endfor %}

  });
       document.getElementById('1-4box-second-Link-produit-1').addEventListener('click', function() {
