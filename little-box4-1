    var Section1quatreBoxWishP4B3 = document.querySelector('.Section1quatreBoxWishP4B3');
    
    
    var Section1Box4ImageProduitBox1 = document.getElementById('Section1Box4ImageProduitBox1');
    var Section1Box4ImageProduitBox2 = document.getElementById('Section1Box4ImageProduitBox2');
    var Section1Box4ImageProduitBox3 = document.getElementById('Section1Box4ImageProduitBox3');
    var Section1Box4ImageProduitBox4 = document.getElementById('Section1Box4ImageProduitBox4');


    var Section1Box4LinkBox1 = document.getElementById('Section1Box4LinkBox1');
    var Section1Box4LinkBox2 = document.getElementById('Section1Box4LinkBox2');
    var Section1Box4LinkBox3 = document.getElementById('Section1Box4LinkBox3');
    var Section1Box4LinkBox4 = document.getElementById('Section1Box4LinkBox4');




    var Section1Box4Img1P1 = document.querySelector('.Section1Box4Img1P1');
    var Section1Box4Img2 = document.querySelector('.Section1Box4Img2');
    var Section1Box4Img3 = document.querySelector('.Section1Box4Img3');

     var Section1Box4Img1P2 = document.querySelector('.Section1Section1Box4Img1P2');
