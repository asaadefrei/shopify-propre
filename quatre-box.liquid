      color: #222222;

    }
     .section-custom-quatre-box h5 {
     color: #222222;
font-size:1rem;

    }
      .section-custom-quatre-box span {
        color: #C2565E; 

    }

      .section-custom-quatre-box .promo {
        text-decoration: line-through;
    }

    .section-custom-quatre-box .sub-flexbox {
      flex: 1;
      width: 50%;
      display: flex;
      flex-direction: row;
      object-fit: contain;

    }

    .section-custom-quatre-box .third-flexbox {
      display: flex;
      flex-direction: column;
      width: 50%;

    }

   
    .section-custom-quatre-box .button-wishlist {
      position: absolute;
        top: 21px;
      right: 15px;
      border:none;
      background-color: transparent;
      outline: none;

    }

       .section-custom-quatre-box .anotation {
      position: absolute;
      top: 8px;
      left: 24px;
      width: 100px;
      font-size: 1.5rem;
        color: #222222;
    }


    .section-custom-quatre-box .inner-flexbox-image {
      height: 600px;
      object-fit:contain;
    }

    .section-custom-quatre-box .inner-flexbox-image img {
      height: 100%;
      width: 100%;
      object-fit:cover;


    }

    .section-custom-quatre-box .inner-flexbox-item {
      display: flex;
      height: 88px;
      flex-direction: row;
      align-items: center;
      justify-content: center;
      align-content: center;
      object-fit:contain;

    }


    .section-custom-quatre-box .inner-flexbox-item .div-element {
      max-height: 100%;
      object-fit:contain;
      width: 100%;
      margin: 12px 16px;
      justify-content: space-between;

    }

    .section-custom-quatre-box .section-1 {
      justify-content: space-between;
      display: flex;

    }

    .section-custom-quatre-box .section-2 {
      justify-content: space-between;
      display: flex;
      align-items: center; /* Align items vertically in the center */
      margin-top: -20px;

    }
 .section-custom-quatre-box .div-button {
        font-size: 0;
      }
    .section-custom-quatre-box .link-block {
      padding-left: 5px;
    }

    .section-custom-quatre-box .cercle {
      border: 1px solid #010101;
      border-radius: 20px;


    }
 

    @media screen and (max-width: 750px) {
      .section-custom-quatre-box .main-flexbox {
        display: flex;
        flex-direction: column;
        width: 100%;
        border: 1px solid #ddd;
      }

      .section-custom-quatre-box .sub-flexbox {
        width: 100%;
        display: flex;
        flex-direction: row;
      }

      .section-custom-quatre-box.third-flexbox {
        width: 100%;
        border: 1px solid #ddd;
        flex-direction: column;
        flex-wrap: wrap;

      }

      

    }

        @media screen and (max-width: 550px) {

